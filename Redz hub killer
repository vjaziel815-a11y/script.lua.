local CorrectKey = "998"

local function MainScript()
    local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

    local Window = Fluent:CreateWindow({
        Title = "Superior Hub | Jorge VIP",
        SubTitle = "üëë REDZ KILLER ACTIVADO",
        TabWidth = 160,
        Size = UDim2.fromOffset(580, 460),
        Acrylic = true, 
        Theme = "Darker",
        MinimizeKey = Enum.KeyCode.LeftControl
    })

    local Tabs = {
        Main = Window:AddTab({ Title = "Farm üöú", Icon = "home" }),
        Combat = Window:AddTab({ Title = "PvP ‚öîÔ∏è", Icon = "sword" }),
        Settings = Window:AddTab({ Title = "Config ‚öôÔ∏è", Icon = "settings" })
    }

    Tabs.Main:AddToggle("AutoFarm", {Title = "Auto Farm Level", Default = false})
    Tabs.Combat:AddSlider("Hitbox", {Title = "Hitbox Size", Default = 30, Min = 0, Max = 100, Rounding = 1, Callback = function(V) _G.HitboxSize = V end})
    
    Window:SelectTab(1)
end

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextBox = Instance.new("TextBox")
local TextButton = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")

ScreenGui.Parent = game.CoreGui
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.Position = UDim2.new(0.5, -110, 0.5, -70)
Frame.Size = UDim2.new(0, 220, 0, 140)
Frame.Active = true
Frame.Draggable = true

TextLabel.Parent = Frame
TextLabel.Size = UDim2.new(1, 0, 0, 40)
TextLabel.Text = "JORGE HUB | KEY: 998"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1

TextBox.Parent = Frame
TextBox.Position = UDim2.new(0.1, 0, 0.35, 0)
TextBox.Size = UDim2.new(0.8, 0, 0.25, 0)
TextBox.PlaceholderText = "Escribe la llave..."
TextBox.Text = ""

TextButton.Parent = Frame
TextButton.Position = UDim2.new(0.2, 0, 0.7, 0)
TextButton.Size = UDim2.new(0.6, 0, 0.2, 0)
TextButton.Text = "ENTRAR"
TextButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)

TextButton.MouseButton1Click:Connect(function()
    if TextBox.Text == CorrectKey then
        ScreenGui:Destroy()
        MainScript()
    else
        TextBox.Text = ""
        TextBox.PlaceholderText = "MAL, ES 998"
    end
end)
