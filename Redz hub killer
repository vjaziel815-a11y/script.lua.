local function MainScript()
    local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

    local Window = Fluent:CreateWindow({
        Title = "Superior Hub | Jorge VIP",
        SubTitle = "üëë KEY SYSTEM ACTIVADO",
        TabWidth = 160,
        Size = UDim2.fromOffset(580, 460),
        Acrylic = true, 
        Theme = "Darker",
        MinimizeKey = Enum.KeyCode.LeftControl
    })

    Fluent:Notify({
        Title = "¬°Acceso Concedido!",
        Content = "Bienvenido Jorge, la llave es correcta.",
        Duration = 5
    })

    local Tabs = {
        Main = Window:AddTab({ Title = "Farm üöú", Icon = "home" }),
        Combat = Window:AddTab({ Title = "PvP God ‚öîÔ∏è", Icon = "sword" }),
        Settings = Window:AddTab({ Title = "Config ‚öôÔ∏è", Icon = "settings" })
    }

    Tabs.Main:AddToggle("AutoFarm", {Title = "Auto Farm Level", Default = false})
    Tabs.Combat:AddSlider("Hitbox", {Title = "Hitbox", Default = 30, Min = 0, Max = 100, Rounding = 1, Callback = function(V) _G.HitboxSize = V end})
    
    Window:SelectTab(1)
end

