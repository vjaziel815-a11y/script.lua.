local function MainScript()
    local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
    local Window = Fluent:CreateWindow({
        Title = "Superior Hub | Jorge VIP",
        SubTitle = "üëë REDZ KILLER ACTIVADO",
        TabWidth = 160,
        Size = UDim2.fromOffset(580, 460),
        Acrylic = true,
        Theme = "Darker"
    })

    local Tabs = { Main = Window:AddTab({ Title = "Farm üöú", Icon = "home" }) }

    Tabs.Main:AddToggle("AutoFarm", {
        Title = "Activar Auto Farm (SIN ERROR)",
        Default = false,
        Callback = function(Value)
            if Value then
                -- ESTE ES EL MOTOR QUE S√ç EST√Å ACTIVO
                loadstring(game:HttpGet("https://raw.githubusercontent.com/realredz/BloxFruits/main/Source.lua"))()
            end
        end
    })
end

